    "@anticrm/plugin-core-dev": "~0.4.0",
    "@anticrm/chunter": "~0.4.0",
    "@anticrm/chunter-assets": "~0.4.0",
    "@anticrm/chunter-impl": "~0.4.0",
    "@anticrm/task": "~0.4.0",
    "@anticrm/task-assets": "~0.4.0",
    "@anticrm/task-impl": "~0.4.0",
