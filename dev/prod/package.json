{
  "name": "prod",
  "version": "1.0.0",
  "license": "EPL-2.0",
  "scripts": {
    "build": "echo 'no build required'",
    "fast-build": "echo 'no build required'",
    "build-demo": "cross-env NODE_ENV=production PUBLIC_PATH=/platform/ webpack && ./build_demo.sh",
    "analyze": "cross-env NODE_ENV=production webpack --json > stats.json",
    "show": "webpack-bundle-analyzer stats.json dist",
    "dev": "cross-env FAST_BUILD=true webpack serve --content-base public",
    "dev-bin": "cross-env NO_SVELTE=true webpack serve --content-base public",
    "dev-server": "cross-env CLIENT=server FAST_BUILD=true webpack serve --content-base public",
    "dev-server-bin": "cross-env CLIENT=server FAST_BUILD=true NO_SVELTE=true webpack serve --content-base public",
    "start": "cross-env CLIENT=server NODE_ENV=production webpack serve --content-base public",
    "test": "echo 'no tests'",
    "lint": "eslint --max-warnings=0 src",
    "lint:fix": "eslint --fix src",
    "deploy": "cp -p public/* dist && aws s3 sync dist s3://anticrm-platform --delete --acl public-read",
    "format": "prettier --write --plugin-search-dir=. src && eslint --fix src"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "webpack-cli": "^4.6.0",
    "webpack": "^5.32.0",
    "mini-css-extract-plugin": "^1.4.1",
    "dotenv-webpack": "^7.0.2",
    "ts-loader": "^8.1.0",
    "svelte-loader": "^3.1.0",
    "css-loader": "^5.2.1",
    "webpack-dev-server": "^3.11.2",
    "style-loader": "^2.0.0",
    "file-loader": "^6.2.0",
    "sass-loader": "^11.0.1",
    "@types/node": "^14.14.40",
    "webpack-bundle-analyzer": "^4.4.1",
    "svgo-loader": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@anticrm/platform": "~0.4.2",
    "@anticrm/status": "~0.4.2",
    "@anticrm/ui": "~0.4.1",
    "svelte": "^3.37.0",
